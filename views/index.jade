doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='description', content='')
    meta(name='author', content='')
    title Ladies of Light
    // Bootstrap Core CSS
    link(href='css/bootstrap.css', rel='stylesheet')
    // Custom CSS
    link(href='css/clean-blog.css', rel='stylesheet')
    // Custom Fonts
    link(href='http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800', rel='stylesheet', type='text/css')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body
    // Navigation
    nav.navbar.navbar-default.navbar-custom.navbar-fixed-top
      .container-fluid
        // Brand and toggle get grouped for better mobile display
        .navbar-header.page-scroll
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='index.html') Laides of LIGHT
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href='/') Home
            li
              a(href='events') Events
            li
              a(href='about') About Us
        // /.navbar-collapse
      // /.container
    // Page Header
    // Set your background image for this header on the line below.
    header.intro-header(style="background-image: url('img/ladiesbgsmall.jpg'); min-width='100%'")
      .container
        .row
          .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
            .site-heading
              img.img-responsive(src="img/ladiesbg.png")
              //h1#home Ladies of LIGHT
              //hr.small
              //span.subheading Love.Integrity.Gratitude.Health.Transformation
      // Main content
    .container
      .row
        .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
          form(action='/my-handling-form-page', method='post')
            h2 Sign up for notifications on upcoming events and more!
            hr
            fieldset.form-group
              label(for='firstname') first name
              br
              input#firstName.form-control(type='text', placeholder='first name')
              br
              label(for='firstname') last name
              br
              input#lastName.form-control(type='text', placeholder='last name')
              br
              label(for='exampleInputEmail1') email
              input#exampleInputEmail1.form-control(type='email', placeholder='you@company.com')
              br
              label(for='zipCode') zip code
              br
              input#addressLine1.form-control(type='number', placeholder='12345')
            fieldset.form-group
              label(for='phone') phone
              input#phone.form-control(type='number', placeholder='optional')
              fieldset.form-group
              label(for='exampleTextarea') leave us a note
              textarea#exampleTextarea.form-control(rows='3')
            br
            button.btn.btn-primary(type='submit') Submit
    // Footer
    footer
      .container
        .row
          .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
            ul.list-inline.text-center
              li
                a(href='http://www.instagram.com/ladies_of_light_la')
                  span.fa-stack.fa-lg
                    i.fa.fa-circle.fa-stack-2x
                    i.fa.fa-instagram.fa-stack-1x.fa-inverse
              li
                a(href='http://www.facebook.com/ladiesoflightla')
                  span.fa-stack.fa-lg
                    i.fa.fa-circle.fa-stack-2x
                    i.fa.fa-facebook.fa-stack-1x.fa-inverse
            p.copyright.text-muted Ladies of Light 2017
            // @ladies_of_light_la
            // 
    // jQuery
    script(src='js/jquery.js')
    // Bootstrap Core JavaScript
    script(src='js/bootstrap.min.js')
    // Custom Theme JavaScript
    script(src='js/clean-blog.min.js')
